@page "/monate"
<h3>Monatsliste</h3>
<div class="container">
    <div class="row">
        <div class="col-12 col-md-6">
            Monat
        </div>
        <div class="col-12 col-md-6 ">
            <ul class="list-group">
                <li class="list-group-item active" aria-current="true">Monate</li>
                @for (int i = 0; i < 12; i++)
                {
                    <li class="list-group-item">@DateTime.Now.AddMonths(i).ToString("MMMM")</li>
                }
            </ul>
        </div>
    </div>

</div>
