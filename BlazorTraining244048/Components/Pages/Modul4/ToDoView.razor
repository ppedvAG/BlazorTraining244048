@page "/todo"
@inject ToDoVM VM
<h3>ToDoView</h3>
<div class="container">
    <div class="row">
        <div class="col">
            <ul class="list-group">
                <li class="list-group-item active" aria-current="true">Todos</li>

                @foreach (var item in VM.TodoO.todos)
                {

                    <li class='list-group-item  @(item.completed?"list-group-item-dark":"") '>
                        @item.todo
                    </li>
                }
            </ul>
        </div>
    </div>

</div>
